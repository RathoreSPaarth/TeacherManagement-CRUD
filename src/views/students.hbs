<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Students Name</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <th scope="row">{{@index}}</th>
      <td>{{this.studentName}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<a class="btn btn-primary btn-lg" href="/add-students/{{searchid}}" role="button">Add Students</a>